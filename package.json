{
    "name": "dwarvesforge-wp-stack-snippets",
    "displayName": "Dwarvesforge WP-Stack Snippets",
    "description": "Dwarvesforge WP-Stack Visual Studio Code Snippets",
    "version": "1.0.2",
    "publisher": "Dwarvesforge",
    "icon": ".resources/icon.png",
    "scripts": {
        "build": "node .scripts/snippets.js",
        "publish": "vsce publish"
    },
    "repository": {
        "type": "git",
